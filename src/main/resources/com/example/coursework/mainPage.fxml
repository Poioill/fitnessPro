<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TreeView?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.coursework.HomeController">
    <children>
        <TreeView fx:id="menu" onContextMenuRequested="#treeMenu" onMouseClicked="#treeMenu" prefHeight="630.0" prefWidth="260.0" />
      <Button fx:id="exitButton" layoutX="14.0" layoutY="591.0" mnemonicParsing="false" onAction="#exit" styleClass="exitBtn" text="Exit" />
      <ImageView fitHeight="65.0" fitWidth="196.0" layoutX="30.0" layoutY="14.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@LOGO.png" />
         </image>
      </ImageView>
    </children>
</AnchorPane>
