<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<?import javafx.scene.image.ImageView?>
<?import javafx.scene.image.Image?>
<?import javafx.geometry.Insets?>
<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="com.example.coursework.HomeController"
            prefHeight="400.0" prefWidth="600.0">
    <children>
        <TreeView fx:id="menu" centerShape="false" nodeOrientation="LEFT_TO_RIGHT" onContextMenuRequested="#treeMenu" onMouseClicked="#treeMenu" prefHeight="630.0" prefWidth="260.0">
            <padding>
                <Insets top=" 80.0" />
            </padding></TreeView>
        <ImageView fitHeight="65.0" fitWidth="196.0" layoutX="32.0" layoutY="14.0" pickOnBounds="true" preserveRatio="true">
            <image>
                <Image url="@LOGO.png" />
            </image>
        </ImageView>
        <Button fx:id="exitButton" layoutX="14.0" layoutY="591.0" mnemonicParsing="false" onAction="#exit" styleClass="exitBtn" text="Exit" />
    </children>
</AnchorPane>
